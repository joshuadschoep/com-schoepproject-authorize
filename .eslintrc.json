{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "files": ["src/**/*.ts", "src/**/*.js", "webpack.config.ts"],
    "rules": {
        "@typescript-eslint/explicit-function-return-type": "off"
    }
}
